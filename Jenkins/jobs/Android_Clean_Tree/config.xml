<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.buildblocker.BuildBlockerProperty plugin="build-blocker-plugin@1.4-SNAPSHOT">
      <useBuildBlocker>true</useBuildBlocker>
      <blockingJobs>Android</blockingJobs>
    </hudson.plugins.buildblocker.BuildBlockerProperty>
    <hudson.queueSorter.PrioritySorterJobProperty plugin="PrioritySorter@1.3">
      <priority>100</priority>
    </hudson.queueSorter.PrioritySorterJobProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>H H * * 0</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>/hd2/android</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>export LANG=en_US.UTF-8

set -e
set +x

arch_envsetup() {
  # JDK and JRE 6 paths
  export PATH=&quot;/opt/java6/jre/bin:/opt/java6/bin:${PATH}&quot;

  unset _JAVA_OPTIONS

  mkdir -p &quot;${WORKSPACE}/bin/&quot;
  if [ ! -f &quot;${WORKSPACE}/bin/python&quot; ]; then
    ln -s /usr/bin/python2 &quot;${WORKSPACE}/bin/python&quot;
  fi
}

arch_envsetup

echo &quot;================================================================================&quot;
echo &quot;Cleaning cyanogenmod_cm-10.2 build tree&quot;
echo &quot;================================================================================&quot;
echo &quot;Disk space before clean:&quot;
df -h
echo &quot;================================================================================&quot;
cd &quot;${WORKSPACE}/cyanogenmod_cm-10.2/&quot;
make clobber || {
  echo &quot;================================================================================&quot;
  echo &quot;FAILED TO RUN \&quot;make clobber\&quot;. Entire out/ directory will be removed.&quot;
  echo &quot;================================================================================&quot;
  rm -rf out/
}
echo &quot;================================================================================&quot;
echo &quot;Disk space after clean:&quot;
df -h
echo &quot;================================================================================&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>